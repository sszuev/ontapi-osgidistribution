    <build>
        <testResources>
            <testResource>
                <directory>src/test/resources</directory>
            </testResource>
        </testResources>

        <plugins>
            <plugin>
                <artifactId>maven-clean-plugin</artifactId>
                <version>3.1.0</version>
                <configuration>
                    <filesets>
                        <fileset>
                            <directory>felix-cache</directory>
                            <includes>
                                <include>**</include>
                            </includes>
                            <followSymlinks>false</followSymlinks>
                        </fileset>
                    </filesets>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.22.2</version>
                <configuration>
                    <includes>
                        <include>**/*Test.java</include>
                    </includes>
                    <excludes>
                        <exclude>**/*ITTest.java</exclude>
                    </excludes>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
                <version>2.22.2</version>
                <configuration>
                    <includes>
                        <include>**/*ITTest.java</include>
                    </includes>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>integration-test</goal>
                            <goal>verify</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>3.2.1</version>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                        <configuration>
                            <artifactSet>
                                <includes>
                                    <include>com.github.owlcs:ontapi</include>
                                    <include>net.sourceforge.owlapi:owlapi-oboformat</include>
                                    <include>net.sourceforge.owlapi:owlapi-parsers</include>
                                    <include>net.sourceforge.owlapi:owlapi-rio</include>
                                    <include>net.sourceforge.owlapi:owlapi-tools</include>
                                </includes>
                            </artifactSet>
                            <transformers>
                                <transformer
                                        implementation="org.apache.maven.plugins.shade.resource.ServicesResourceTransformer"/>
                            </transformers>
                            <createSourcesJar>true</createSourcesJar>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>4.1.0</version>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Implementation-Title>${project.name}</Implementation-Title>
                        <Implementation-Vendor>${project.organization.name}</Implementation-Vendor>
                        <Implementation-Version>${project.version}.${maven.build.timestamp}</Implementation-Version>
                        <Bundle-SymbolicName>com.github.owlcs.ontapi-distribution-bundle</Bundle-SymbolicName>
                        <Bundle-Version>${project.version}</Bundle-Version>
                        <Embed-Directory>lib</Embed-Directory>

                        <!-- todo: a lot of warnings about privates: -->
                        <_exportcontents>
                            org.apache.jena.*;version=${version.jena},
                            com.clarkparsia.*,
                            org.semanticweb.owlapi.*;version=${version.owlapi};-split-package:=merge-first,
                            org.semanticweb.owlapitools.*;version=${version.owlapi};-split-package:=merge-first,
                            uk.ac.manchester.*;version=${version.owlapi};-split-package:=merge-first,
                            org.obolibrary.*;version=${version.owlapi};-split-package:=merge-first,
                            com.github.owlcs.*;version=${version.ontapi}
                        </_exportcontents>

                        <Embed-Dependency>
                            groupId=com.github.owlcs;artifactId=ontapi;scope=compile|runtime|provided;inline=true,
                            groupId=net.sourceforge.owlapi;scope=compile|runtime|provided;inline=true,

                            artifactId=slf4j-api;scope=compile|runtime|provided;inline=false,
                            groupId=org.apache.jena;scope=compile|runtime|provided;inline=false,

                            artifactId=commons-rdf-api;scope=compile|runtime|provided;inline=false,
                            groupId=commons-codec;scope=compile|runtime|provided;inline=false,
                            groupId=org.apache.commons;scope=compile|runtime|provided;inline=false,
                            groupId=commons-io;scope=compile|runtime|provided;inline=false,
                            groupId=com.github.andrewoma.dexx;scope=compile|runtime|provided;inline=false,
                            groupId=com.github.jsonld-java;scope=compile|runtime|provided;inline=false,
                            groupId=org.apache.thrift;scope=compile|runtime|provided;inline=false,
