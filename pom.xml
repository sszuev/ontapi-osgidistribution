    <artifactId>ontapi-osgidistribution</artifactId>
    <version>1.0.0-SNAPSHOT</version>

    <packaging>bundle</packaging>

    <name>ONT-API + OWL-API Tools &amp; Formats - OSGi and Distribution</name>

    <properties>
        <maven.compiler.target>1.8</maven.compiler.target>
        <maven.compiler.source>1.8</maven.compiler.source>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <version.ontapi>2.0.1-SNAPSHOT</version.ontapi>
        <version.jena>3.12.0</version.jena>
        <version.owlapi>5.1.11</version.owlapi>
        <version.caffeine>2.8.0</version.caffeine>
        <version.slf4j>1.7.28</version.slf4j>

        <!-- used by jena-arq -->
        <ver.httpclient>4.5.5</ver.httpclient>
        <ver.httpcore>4.4.9</ver.httpcore>
        <!-- normally, these are the same as the above -->
        <version.httpcore-osgi>${ver.httpcore}</version.httpcore-osgi>
        <version.httpclient-osgi>${ver.httpclient}</version.httpclient-osgi>
        <!-- jena-arq, jena-base, utils -->
        <version.commons-lang3>3.4</version.commons-lang3>
        <!-- jena-base, jena-core -->
        <version.commons-io>2.6</version.commons-io>
        <!-- jena-base, sparql resultsets -->
        <version.commons-csv>1.5</version.commons-csv>
        <!-- jena-base (sparql), jena-base -->
        <version.dexx-collection>0.7</version.dexx-collection>
        <!-- used by jena and owlapi-rio  -->
        <version.jsonld-java>0.12.3</version.jsonld-java>
        <!-- jena-arq, bin-rdf -->
        <version.libthrift>0.12.0</version.libthrift>
        <!-- used by jsonld, jena (jackson-core,jackson-databind) & owl-api (+jackson-annotations) -->
        <version.jackson>2.9.10</version.jackson>
        <version.jackson-databind>2.9.10.3</version.jackson-databind>
        <!-- guava depends only on several packs with annotations -->
        <version.google-guava>22.0</version.google-guava>
        <!-- javax.inject is used by owl-api, even by owlapi-api -->
        <version.bundles-javax-inject>1_3</version.bundles-javax-inject>
        <!-- used by owlapi-api (e.g. org.semanticweb.owlapi.model.IRI) -->
        <version.commons-rdf-api>0.5.0</version.commons-rdf-api>
        <!-- owlapi-api, org.semanticweb.owlapi.io.RDFTriple -->
        <version.vsonnier-hppcrt>0.7.5</version.vsonnier-hppcrt>
        <!-- owlapi-api, org.semanticweb.owlapi.io.DocumentSources -->
        <version.tukaani-xz>1.8</version.tukaani-xz>
        <!-- by jena-core, XSDbase64Binary -->
        <version.commons-codec>1.11</version.commons-codec>
        <!-- jena-base, io -->
        <version.commons-compress>1.19</version.commons-compress>
        <!--httpclient-cache is not explicitly used by jena (just as utility in org.apache.jena.riot.web.HttpOp) -->
        <version.httpclient-cache>4.5.5</version.httpclient-cache>
        <!-- owlapi, javax.annotations -->
        <version.findbug-jsr305>3.0.2</version.findbug-jsr305>
        <!-- owlapi-parsers -->
        <version.eclipse-rdf4j>2.3.2</version.eclipse-rdf4j>
    </properties>

    <repositories>
        <repository>
            <id>snapshots-repo</id>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>

    <profiles>
        <profile>
            <id>withDefaultImpl</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>net.sourceforge.owlapi</groupId>
                    <artifactId>owlapi-impl</artifactId>
                    <version>${version.owlapi}</version>
                    <exclusions>
                        <exclusion>
                            <groupId>*</groupId>
                            <artifactId>*</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-failsafe-plugin</artifactId>
                        <configuration>
                            <systemPropertyVariables>
                                <profile>withDefaultImpl</profile>
                            </systemPropertyVariables>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

